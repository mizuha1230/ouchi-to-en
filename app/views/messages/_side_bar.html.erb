<div class="side-bar-header">
  <div class="header-name">
    <a href="#">保護者の名前</a>
  </div>
  <div class="create-room">
    <%= link_to "ルームを作成する",new_room_path, class:"btn btn-light" %>
  </div>
</div>

<div class="rooms">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
        <a class="btn btn-outline-dark" href="#"><%= room.room_name %></a>
      </div>
    </div>
  <% end %>
</div>